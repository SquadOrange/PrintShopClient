
  <!-- {{> partial}} -->

  <table class="cart-handlebars" data-id="{{print.id}}">
    <thead>
      <tr>
        <th>Print</th>
        <th>Quantity</th>
      </tr>
    </thead>

    <tbody>
      {{#each prints as |print|}}
      <tr>
        <td>{{print.title}}</td>
        <td>{{print.quantity}}</td>
        <td>
          <input type="number" name="print[quantity]" placeholder="0" min="0" max="10">
        </td>
        <td>
          <button data-id="{{print.id}}">edit</button>
        </td>
        <td>
          <button class="remove-print-button" data-id="{{print.id}}">remove</button>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
